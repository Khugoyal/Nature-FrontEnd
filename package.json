{
  "name": "natours",
  "version": "1.0.0",
  "description": "landing Page",
  "main": "index.js",
  "scripts": {
    "compile:sass": "node-sass sass/main.scss sass/style.comp.css",
    "concat:css": "concat -o sass/style.concat.css sass/icon-font.css sass/main.css",
    "browserslist": "last 10 versions",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" sass/style.concat.css -o sass/style.prefix.css",
    "compress:css": "node-sass sass/style.prefix.css sass/main.css --output-style compressed"
  },
  "author": "Kanishth",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  },
  "dependencies": {}
}
